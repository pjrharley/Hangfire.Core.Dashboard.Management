<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <IsPackable>true</IsPackable>
    <PackageOutputPath>../artifacts</PackageOutputPath>
    <PackageId>com.mycompliance.hangfire.dashboard.management</PackageId>
    <IncludeContentInPack>true</IncludeContentInPack>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Hangfire.AspNetCore" Version="1.7.6" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Content\management.js" />
    <EmbeddedResource Include="Content\management.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>
</Project>